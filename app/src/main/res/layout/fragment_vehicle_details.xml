<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".VehicleDetailsFragment"
        >

    <TextView
            android:text="Pluged Number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginStart="24dp"/>

    <TextView
            android:text="Type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="Category"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="Production Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="Crossed Out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/textView14"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="Registration Date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/textView13"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="Driver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_vehicle_pluged_number"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="5dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_vehicle_driver"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView_vehicle_pluged_number"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_vehicle_type"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView_vehicle_driver"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_vehicle_category"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView_vehicle_type"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_prod_date"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView_vehicle_category"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView_register_date"
            app:layout_constraintStart_toEndOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/textView_prod_date"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"/>

    <Button
            android:text="Cross Out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button_cross_out"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

    <Button
            android:text="Uncross Out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button_uncross_out"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.581"
            />

    <ProgressBar
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/progressBar_vehicle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginBottom="204dp"
            tools:visibility="visible"
            android:visibility="invisible"/>

</androidx.constraintlayout.widget.ConstraintLayout>